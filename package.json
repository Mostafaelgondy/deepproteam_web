{
  "name": "deepproteam-marketplace",
  "version": "1.0.0",
  "description": "Dealership Marketplace Platform",
  "main": "index.js",
  "scripts": {
    "dev": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/output.css --watch",
    "build": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/output.css --minify",
    "build-admin": "NODE_ENV=production npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/output.css --minify",
    "start": "npx serve .",
    "dev:full": "concurrently \"npm run dev\" \"npm run start\""
  },
  "keywords": [
    "marketplace",
    "admin-dashboard",
    "ecommerce"
  ],
  "author": "Deepproteam",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "chart.js": "^4.3.0",
    "flatpickr": "^4.6.13"
  }
}